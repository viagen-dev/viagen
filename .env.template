# Environment variables for Viagen
# Copy this file to .env and fill in values

ANTHROPIC_API_KEY=

# Optional: auth token for sandbox/remote access
# VIAGEN_AUTH_TOKEN=

# GitHub PAT for git-based sandbox (enables commit+push from sandbox)
# GITHUB_TOKEN=

# Vercel Sandbox (run `vercel link && vercel env pull` or set manually)
# VERCEL_TOKEN=
# VERCEL_TEAM_ID=
# VERCEL_PROJECT_ID=
